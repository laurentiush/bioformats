<?xml version="1.0" encoding="UTF-8"?>
<configuration>
  <appender name="stdout" class="ch.qos.logback.core.ConsoleAppender">
    <encoder>
      <pattern>%d [%t] %-5p %c - %m%n</pattern>
    </encoder>
  </appender>
  <logger name="loci.formats.ClassList" level="FATAL"/>
  <root level="WARN">
    <appender-ref ref="stdout"/>
  </root>
</configuration>
